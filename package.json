{
  "name": "report-counts",
  "version": "0.2.5",
  "description": "Plugin for generating report counts.",
  "main": "./dist/bundle.js",
  "scripts": {
    "analyze": "webpack --analyze",
    "clean": "rm -rf dist junit bundle-stats.json",
    "lint:js": "eslint --ext .js --ext .vue ./*.js js js-tests",
    "prebuild": "npm install",
    "build": "webpack --mode=production",
    "build-dev": "webpack --mode=development",
    "test": "karma start karma.conf.js --single-run",
    "test-ci": "karma start karma.conf.js --single-run --browsers Chrome_Headless",
    "test-jenkins": "karma start karma.conf.js --single-run --reporters junit --browsers Chrome_Headless",
    "test-single-run": "karma start karma.conf.js --single-run",
    "start": "WATCHING=1 webpack --mode=development --progress --colors --watch"
  },
  "author": "BMIP - OCTRI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://octriinternal.ohsu.edu/stash/scm/reddev/report-counts.git"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@vue/test-utils": "^1.3.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^9.1.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.23.1",
    "eslint-plugin-vue": "^7.20.0",
    "file-loader": "^6.2.0",
    "git-repo-info": "^2.1.1",
    "html-webpack-plugin": "^5.5.0",
    "jasmine-ajax": "^4.0.0",
    "jasmine-core": "^4.4.0",
    "jquery": "^3.6.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "0.0.33",
    "karma-webpack": "^5.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "moment": "^2.29.4",
    "postcss": "^8.4.6",
    "style-loader": "^3.3.1",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.16.7",
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "qs": "^6.10.3",
    "uuid": "^8.3.2",
    "vue": "^2.6.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
